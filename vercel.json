{
  "version": 2,
  "builds": [
    {
      "src": "index.htm",
      "use": "@vercel/static"
    },
    {
      "src": "api/matches.php",
      "use": "@vercel/php"
    }
  ],
  "routes": [
    {
      "src": "/api/matches.php",
      "dest": "/api/matches.php"
    },
    {
      "src": "/(.*)",
      "dest": "/index.htm"
    }
  ]
}
